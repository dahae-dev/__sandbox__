    "start-inspect-babel": "cross-env NODE_ENV=development BABEL_ENV=node babel-node --inspect --",
    "lint": "eslint --",
    "build": "cross-env BABEL_ENV=node babel --out-dir lib --",
    "webpack": "webpack --env.dev --config __config__/webpack.config.js",
    "webpack:prod": "webpack --config __config__/webpack.config.js",
    "dev": "cross-env NODE_ENV=development nodemon --require @babel/register --",
    "devn": "cross-env NODE_ENV=development nodemon --no-warnings --experimental-modules --es-module-specifier-resolution=node --experimental-json-modules --nolazy --inspect-brk --",
    "devi": "cross-env NODE_ENV=development nodemon --exec 'babel-node --nolazy --inspect-brk' --",
    "devt": "cross-env NODE_ENV=development TS_NODE_FILES=true nodemon --watch '**/*.ts' --exec 'node --require ts-node/register --nolazy' --",
    "devti": "cross-env NODE_ENV=development TS_NODE_FILES=true nodemon --watch '**/*.ts' --exec 'node --require ts-node/register --nolazy --inspect-brk' --",
    "ndb": "cross-env NODE_ENV=development ndb --require @babel/register",
    "package": "pkg --target node10-macos --output build/sandbox lib/index.js",
    "__old__dev-inspect": "cross-env BABEL_ENV=node nodemon --inspect --require esm --require @babel/register --",
    "__old__dev-inspect-brk": "cross-env BABEL_ENV=node nodemon --exec 'node --nolazy --require esm --require @babel/register --inspect-brk' --",
    "__old-ndb": "cross-env BABEL_ENV=node ndb nodemon --exec 'node --require esm --require @babel/register' --"
