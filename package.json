    "startsi": "cross-env TS_NODE_FILES=true NODE_ENV=development node --require ts-node/register --nolazy --inspect-brk --",
    "start": "cross-env NODE_ENV=development node --require source-map-support/register --",
    "start-inspect": "cross-env NODE_ENV=development node --nolazy --inspect-brk --require source-map-support/register --",
    "startm": "cross-env NODE_ENV=development node --no-warnings --experimental-modules --es-module-specifier-resolution=node --experimental-json-modules --",
